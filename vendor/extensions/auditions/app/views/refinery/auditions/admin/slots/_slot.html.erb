<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(slot) -%>">
  <span class='title'>
    <%= slot.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_auditions_admin_slot_path(slot),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.auditions_admin_slot_path(slot),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => slot.title),
        :method => :delete %>
  </span>
</li>
